{
  "name": "vector-chunk",
  "version": "1.0.0",
  "description": "Lightweight, dependency-free vector processing and chunking package optimized for Elasticsearch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:prod": "node build.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build:prod && npm test",
    "start": "node examples/basic-usage.ts",
    "example:advanced": "node examples/advanced-optimizations.ts",
    "benchmark": "node src/benchmarks/performance-benchmark.ts",
    "benchmark:quick": "node -e \"const { createPerformanceBenchmark } = require('./dist/benchmarks/performance-benchmark'); const b = createPerformanceBenchmark({textSize: 10000, chunkSize: 256, strategy: 'fixed'}); b.runBenchmark().then(() => b.printResults());\""
  },
  "keywords": [
    "vector",
    "chunking",
    "elasticsearch",
    "embeddings",
    "nlp",
    "text-processing",
    "machine-learning"
  ],
  "author": "Nedunchezhiyan M",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nedunchezhiyan-M/vector-chunk.git"
  },
  "bugs": {
    "url": "https://github.com/Nedunchezhiyan-M/vector-chunk/issues"
  },
  "homepage": "https://github.com/Nedunchezhiyan-M/vector-chunk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
